{
    "compilerOptions": {
      "target": "es2018",
      "module": "commonjs",
       "outDir": "./build",
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "baseUrl": "."    
    },
    "include": [
      "./**/*",
    ]
  }
  